def ReadFile(file, state):
    for line in file:
        line = line.strip()
        line = line.split(' ')
        state["male"]["name"].append(line[1])
        state["male"]["number"].append(line[2])
        state["female"]["name"].append(line[3])
        state["female"]["number"].append(line[4])

ReadFile(FLnames, FLinput)
ReadFile(CAnames, CAinput)